{
  "servers": {
    "ado": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@azure-devops/mcp", "msdata"],
      "env": {
        "ADO_DEFAULT_PROJECT": "SQLToolsAndLibraries",
        "ADO_DEFAULT_REPO": "ScriptDOM",
        "ADO_DEFAULT_BRANCH": "main",
        "ADO_DEFAULT_AREA_PATH": "SQLToolsAndLibraries\\DacFx"
      }
    },
    "my-mcp-mini-drivers": {
			"url": "https://ecmini.azurewebsites.net",
			"type": "http",
			"headers": {
				"x-mcp-ec-organization": "msdata",
				"x-mcp-ec-project": "SQLToolsAndLibraries",
				"x-mcp-ec-repository": "ScriptDOM",
				"x-mcp-ec-branch": "main"
			}
		} 
  }
}